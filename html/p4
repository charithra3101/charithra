    <script>
        function lab3(){
            var n;
            n=parseInt(prompt("Enter a number"))
            msg="The first "+ n + "Numbers are \n"
            for(i=1;i<=n;i++){
                msg+=i + "\n";

            }
            alert(msg);
        }
    </script>
</head>
<body>
    <h1 onclick="lab3();">Click</h1>
</body>
</html>


var n;
n=prompt("Enter a number")
document.write("The first "+n + " Numbers are <br />")
for(i=0;i<=n;i++)
{
    document.write(i +"<br />")
}


var n=prompt("Enter a number");
f1=0;
f2=1;
//console.log(f1 + "\n" + f2);
document.write("The fibonacci series upto " +n+ "is <br />");
document.write(f1 + "<br />" + f2 + "<br />");

for(i=3;i<=n;i++){
    f3=f1+f2;
    document.write(f3 + "<br />");
    f1=f2;
    f2=f3;
}

//Program to find the sum of First N numbers
var n;
var sum=0;
m="";
n=prompt("Enter a number");
for(i=1;i<=n;i++)
{
    sum+=i;
    m+=i + "\n";
}
alert("sum is \n" + m + "=" + sum);
